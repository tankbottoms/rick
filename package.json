{
  "name": "@jbx-protocol/contracts-v2",
  "bugs": {
    "url": "https://github.com/jbx-protocol/juice-contracts-v2/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jbx-protocol/juice-contracts-v2"
  },
  "version": "0.0.6",
  "license": "MIT",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.1",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/hardhat-upgrades": "^1.8.2",
    "@typechain/hardhat": "^2.0.2",
    "chai": "^4.3.4",
    "eslint": "^8.13.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.6",
    "hardhat": "^2.3.0",
    "hardhat-gas-reporter": "^1.0.4",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.1.6",
    "@openzeppelin/contracts": "^4.5.0-rc.0",
    "@paulrberg/contracts": "^3.4.0",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "glob": "^7.2.0",
    "hardhat-deploy": "^0.11.4",
    "prettier": "^2.4.0",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "rimraf": "^3.0.2",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5"
  },
  "scripts": {
    "chain": "hardhat node",
    "accounts": "hardhat accounts",
    "test": "mocha './test/**/*.test.js' -r esm --bail",
    "clean": "rimraf ./cache && rimraf ./artifacts",
    "compile": "yarn clean && hardhat compile",
    "pretty": "prettier --write \"./**/*.{js,jsx,json,sol}\"",
    "dev": "npx hardhat deploy",
    "dev-memory": "NODE_OPTIONS=--max_old_space_size=8192",
    "deploy:rinkeby": "hardhat deploy --network rinkeby",
    "deploy:kovan": "hardhat deploy --network kovan",
    "deploy:mainnet": "hardhat deploy --network mainnet",
    "deploy:mumbai": "hardhat deploy --network mumbai",
    "verify:rinkeby": "hardhat etherscan-verify --network rinkeby",
    "verify:kovan": "hardhat etherscan-verify --network kovan",
    "verify:mainnet": "hardhat etherscan-verify --network mainnet"
  },
  "packageManager": "yarn@3.2.0"
}
